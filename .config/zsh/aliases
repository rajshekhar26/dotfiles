alias config="/usr/bin/git --git-dir=$HOME/Projects/dotfiles/ --work-tree=$HOME"
alias ls="ls --group-directories-first --human-readable --color=auto"
alias ll="ls -la"
alias la="ls -a"
alias s="sudo"
alias c="clear"
alias v="nvim"
alias vi="nvim"
alias vim="nvim"
alias gcl="git clone"
alias ga="git add"
alias gpl="git pull"
alias gs="git status"
alias gph="git push"
alias gcm="git commit -m"
alias gch="git checkout"
alias gdf="git diff"
alias gbr="git branch"
alias glg="git log"
alias ka="killall"
alias rf="rm -rf"
